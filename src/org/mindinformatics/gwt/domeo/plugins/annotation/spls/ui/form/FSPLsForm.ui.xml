	<ui:style>
	.outer {
		height: 30px;
		padding-left: 0px;
		padding-top: 0px;
		width: 100%;
	}
	
	.buttonsPanel {
		position: absolute;
		right: 20px;
		float: right;
		padding-top: 4px;
		padding-right: 0px;
		padding-left: 2px;
		height: 0px;
		max-height: 20px;
		display: block;
	}
	
	.setPanel {
		padding-left: 5px;
	}
	
	.leftColumn {
		width: 400px;
		vertical-align: top;
	}
	
	.centerColumn {
		width: 330px;
		vertical-align: top;
	}
	
	.titlebarcenter {
		width: 400px;
		background-color: #DDDDDD;
		border-top: 3px solid white;
	}
	
	.rightColumn {
		width: 330px;
		vertical-align: top;
	}
	
	.titlebarright {
		width: 400px;
		background-color: #DDDDDD;
		border-top: 3px solid white;
	}
	
	.panel {
		width: 100%;
		vertical-align: top;
		padding-top: 8px;
	}
	
	.newQualifierPanel {
		min-height: 26px;
		display: inline;
	}
	
	.inner {
		padding-top: 3px;
		padding-bottom: 3px;
		cursor: pointer;
		cursor: hand;
	}
	
	.inner2 {
		padding-top: 3px;
		padding-bottom: 3px;
		padding-left: 3px;
	}
	
	.qualifiersLabel {
		padding-right: 3px;
		background-color: #DDDDDD;
		border-top: 3px solid white;
	}
	
	.titlebar {
		width: 400px;
		background-color: #DDDDDD;
		border-top: 3px solid white;
	}
	
	.commentBody {
		padding-top: 3px;
		padding-left: 3px;
		color: #333;
		font-style: normal;
		width: 360px;
		height: 60px;
		font-size: 12px;
	}
	
	.alleles {
		padding-top: 3px;
		padding-left: 3px;
		color: #333;
		font-style: normal;
		width: 100px;
		height: 13px;
		font-size: 12px;
	}
	
	.listbox1 {
		width: 120px;
	}
	
	.listbox2 {
		width: 200px;
	}
	
	.medical {
		padding-top: 3px;
		padding-left: 3px;
		color: #333;
		font-style: normal;
		width: 125px;
		height: 13px;
		font-size: 12px;
	}
	
	.elementType {
		padding-right: 2px;
		display: inline;
		white-space: nowrap;
	}
	
	.titleLeft {
		padding-left: 4px;
	}
	</ui:style>

